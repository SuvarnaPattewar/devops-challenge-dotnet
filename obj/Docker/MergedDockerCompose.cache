    - C:\Users\suvarnaadmin\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\suvarnaadmin\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
